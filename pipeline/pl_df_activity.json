{
	"name": "pl_df_activity",
	"properties": {
		"activities": [
			{
				"name": "Data flow for ETL",
				"type": "ExecuteDataFlow",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataflow": {
						"referenceName": "df_ELT_sql_to_synapse",
						"type": "DataFlowReference",
						"datasetParameters": {
							"dfsource": {
								"schema": {
									"value": "@pipeline().parameters.source_schema",
									"type": "Expression"
								},
								"table": {
									"value": "@pipeline().parameters.table",
									"type": "Expression"
								}
							},
							"dfsink": {
								"schema": {
									"value": "@pipeline().parameters.sink_schema",
									"type": "Expression"
								},
								"table": {
									"value": "@pipeline().parameters.table",
									"type": "Expression"
								}
							}
						}
					},
					"staging": {
						"linkedService": {
							"referenceName": "test-project-sa-WorkspaceDefaultStorage",
							"type": "LinkedServiceReference"
						},
						"folderPath": "synapsecontainers"
					},
					"compute": {
						"coreCount": 8,
						"computeType": "General"
					},
					"traceLevel": "Fine"
				}
			}
		],
		"parameters": {
			"source_schema": {
				"type": "string",
				"defaultValue": "salesLT"
			},
			"sink_schema": {
				"type": "string",
				"defaultValue": "fact"
			},
			"table": {
				"type": "string"
			}
		},
		"annotations": []
	}
}